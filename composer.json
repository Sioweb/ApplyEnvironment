{
    "name": "sioweb/apply_environment",
    "type": "contao-bundle",
    "description": "Setup environment Data",
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "Sascha Weidner",
            "homepage": "https://www.sioweb.de"
        }
    ],
    "require": {
        "php": "^7.1||^8.0",
        "sioweb/ccevent": "*",
        "contao/core-bundle": "~4.4||~5.0"
    },
    "autoload": {
        "psr-4": {
            "Sioweb\\ApplyEnvironment\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "contao-manager-plugin": "Sioweb\\ApplyEnvironment\\ContaoManager\\Plugin"
    },
    "scripts": {
        "package-scripts": [
            "Sioweb\\ApplyEnvironment\\Composer\\Git::init --repository=https://github.com/Sioweb/ApplyEnvironment"
        ]
    }
}
